<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="472.0" prefWidth="158.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.elmoumene.expense.note.controller.entity.EntityOverviewController">
   <children>
      <BorderPane prefHeight="561.0" prefWidth="240.0">
         <center>
            <TableView fx:id="entityTable" prefHeight="584.0" prefWidth="291.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="316.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button mnemonicParsing="false" onAction="#handleNewEntity" prefHeight="25.0" prefWidth="104.0" text="New" />
                  <Button mnemonicParsing="false" onAction="#handleEditEntity" text="Edit" />
                  <Button mnemonicParsing="false" onAction="#handelDelete" text="Delete" />
              </buttons>
            </ButtonBar>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
