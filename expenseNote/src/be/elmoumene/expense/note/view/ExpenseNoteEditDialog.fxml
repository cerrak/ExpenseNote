      <Label layoutX="516.0" layoutY="27.0" text="Total Amount :" />
      <Label fx:id="amount" layoutX="595.0" layoutY="27.0" prefHeight="17.0" prefWidth="45.0" text="Amount" />
      <Label layoutX="156.0" layoutY="63.0" text="Comments :" />
      <TextField fx:id="commentField" alignment="TOP_LEFT" layoutX="264.0" layoutY="58.0" prefHeight="54.0" prefWidth="210.0" />
   </children>
</AnchorPane>
