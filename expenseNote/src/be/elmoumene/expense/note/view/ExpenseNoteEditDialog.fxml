      <JFXListView fx:id="selectedExpenses" layoutX="456.0" layoutY="152.0" prefHeight="400.0" prefWidth="248.0" AnchorPane.leftAnchor="456.0" />
      <Button layoutX="380.0" layoutY="292.0" mnemonicParsing="false" onAction="#addExpenses" text="&gt;" />
      <Button layoutX="380.0" layoutY="327.0" mnemonicParsing="false" onAction="#removeExpenses" text="&lt;" />
